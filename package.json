{
  "name": "@wmakeev/highland-tools",
  "version": "1.8.0",
  "description": "Tools for highland.js library",
  "keywords": [
    "highland",
    "highlandjs",
    "stream",
    "tools"
  ],
  "files": [
    "build/src/"
  ],
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "scripts": {
    "compile": "tsc",
    "compile:prod": "rm -rf build/ && tsc --build tsconfig.prod.json",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "npm run lint:fix && npm run compile",
    "test": "npm run build && tape build/test",
    "build:prod": "rm -rf build/ && npm run lint:fix && npm run compile:prod",
    "npm:publish": "npm run test && npm run build:prod && npm publish"
  },
  "author": "Vitaliy V. Makeev",
  "license": "MIT",
  "repository": {
    "type": "GitHub",
    "url": "https://github.com/wmakeev/highland-tools"
  },
  "devDependencies": {
    "@types/highland": "^2.12.14",
    "@types/node": "^16",
    "@types/tape": "^4.13.2",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "@typescript-eslint/typescript-estree": "^5.43.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "highland": "^2.13.5",
    "prettier": "^2.7.1",
    "tape": "^5.6.1",
    "typescript": "^4.9.3",
    "typescript-eslint-language-service": "^5.0.0"
  }
}
